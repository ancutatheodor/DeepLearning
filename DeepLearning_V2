t.circle(5,360)
t.end_fill()

t.penup()
t.goto(115,135)
t.begin_fill()
t.pendown()
t.circle(5,360)
t.end_fill()

t.up()
t.goto(55,125)
t.down()
t.seth(-45)
t.color('cyan')
t.circle(60,90)
t.color('orange')
t.circle(30,90)
t.color('cyan')
t.circle(60,90)
t.color('orange')
t.circle(30,90)
t.penup()

t.goto(100,65)
t.pendown()
t.begin_fill()
t.circle(10,360)
t.end_fill()

t.color("white")



